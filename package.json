{
  "private": false,
  "name": "user-service",
  "version": "1.0.0",
  "description": "User service",
  "publishConfig": {
    "registry": "http://localhost:4873/"
  },
  "type": "module",
  "scripts": {
    "migrate": "drizzle-kit generate",
    "unmigrate": "drizzle-kit drop",
    "build:interface": "generate-service-interface -f next",
    "publish:interface": "npm run build:interface && npm publish ./dist",
    "prebuild": "ts-unused-exports tsconfig.json --excludePathsFromReport='app;.config;instrumentation;schema;models'",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "@omer-x/next-openapi-json-generator": "^0.1.0",
    "@omer-x/next-swagger-jsdoc": "^0.1.2",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.0",
    "drizzle-zod": "^0.5.1",
    "next": "^14.2.3",
    "pg": "^8.11.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^14.2.3",
    "@omer-x/eslint-config": "^1.0.7",
    "@omer-x/next-openapi-interface-generator": "^0.5.0",
    "@types/node": "20.12.13",
    "@types/pg": "^8.11.6",
    "@types/react": "18.3.3",
    "drizzle-kit": "^0.22.0",
    "eslint": "^8.57.0",
    "ts-unused-exports": "^10.1.0",
    "typescript": "5.4.5"
  }
}
